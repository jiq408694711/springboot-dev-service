package com.chengdu.jiq.drools1
import java.util.Map;
import com.chengdu.jiq.model.bo.UserDataModel;
import com.chengdu.jiq.model.bo.RuleModel
global java.util.List myGlobalList;
template "tmp1"
rule "drools_@{ruleId}"
    when
        $userData : UserDataModel(@{ifCondition})
        $ruleMap : Map()
    then
        myGlobalList.addAll(((RuleModel)$ruleMap.get("@{ruleId}")));
        System.out.println("规则中打印日志：校验通过!");
end
end template